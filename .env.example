DATASET=GLBX.MDP3
SCHEMA=ohlcv-1m
SYMBOL=NQZ5
MIN_DISPLACEMENT_TICKS=8
MAX_LOOKBACK_MIN=200
# --- Optional session levels (all ET) ---
INCLUDE_ASIA_LONDON=1
